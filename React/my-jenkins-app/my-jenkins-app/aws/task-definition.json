{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "task-definition",
    "containerDefinitions": [
        {
            "name": "myNewImage",
            "image": "nginx:1.27-alpine",
            "portMappings": [
                {
                    "name": "ngnix-80-tcp",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": ""
}